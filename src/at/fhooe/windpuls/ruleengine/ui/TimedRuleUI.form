<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="at.fhooe.windpuls.ruleengine.ui.TimedRuleUI">
  <grid id="27dc6" binding="timedRulePanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="bevel-raised" title="Timed Rule"/>
    <children>
      <grid id="f4aa2" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="West"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="7b92" class="javax.swing.JLabel" binding="frameLab">
            <constraints/>
            <properties>
              <text value="Frame"/>
            </properties>
          </component>
          <component id="939d7" class="javax.swing.JTextField" binding="frameTex">
            <constraints/>
            <properties>
              <columns value="10"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="fe162" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="East"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="bd746" class="javax.swing.JButton" binding="addRuleBut" custom-create="true">
            <constraints/>
            <properties>
              <text value="Add Rule"/>
            </properties>
          </component>
        </children>
      </grid>
      <scrollpane id="6f74d" binding="rulesScroll" custom-create="true">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="3664e" binding="rulesPanel" custom-create="true" layout-manager="GridBagLayout">
            <constraints/>
            <properties/>
            <border type="line" title="Contained Rules"/>
            <children/>
          </grid>
        </children>
      </scrollpane>
    </children>
  </grid>
</form>
